/*키보드로 정수 2개 입력 크기 비교
 * 
 * 
 * 
 * 
 */
public class e3 {

	public static void main(String[] args) {
		Scanner scn = new Scanner (System.in);
		
		System.out.print("정수1:");
		int a=scn.nextInt();
		
		System.out.print("정수2:");
		int b=scn.nextInt();
		
		if(a>b)
			System.out.println("a>b");
		
		if(a==b)
			System.out.println("a=b");
		
		if(a<b)
			System.out.println("a<b");
		
	
	}

}
