import java.util.Scanner;

/*	keyboard input
 * 	50~90  input	10 things     output sum
 */
public class e8 {

	public static void main(String[] args) {
	
		Scanner scn = new Scanner(System.in);
		
		System.out.print("50~90 사이의 정수:");
		int a= scn.nextInt();
		
		int s=0;
		int i=0;
		
		while(s<90 && s>50)	{
			s+=a;
			i++;
		}
		
		if(i==10)
			break;
		}
		System.out.print(s);
	}
}


