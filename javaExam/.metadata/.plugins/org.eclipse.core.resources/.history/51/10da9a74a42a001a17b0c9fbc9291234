/*		정수 1개 입력
 * 'A' ~ 'F' 판단하기
 * 
 */
public class e {

	public static void main(String[] args) {
		int a=90;
		
		if (a>=90)
			System.out.println("A");
		else if (a>=80 && a<90)
			System.out.println("B");
		else if (a>=70 && a<80)
			System.out.println("C");
		else if (a>=60 && a<70)
			System.out.println("D");
		else if (a>=50 && a<60)
			System.out.println("F");
			
	}

}
