/* if ()
 * switch ~ case
 * 
 */
public class e1 {

	public static void main(String[] args) {
		int var=77;
		String str="";	//String : 문자열 처리시
		char ch='A';
		if(var<=100 && var>=90)	{
			str="A";
		}	
		
		else if(var<89 && var>=80);	{
			str="B";
		}
		
		else if(var<79 && var>=70);	{
			str="C";
		}

		else if(var<69 && var>=60);	{
			str="D";
		}
		
		else if(var<60);	{
			str="F";
		}
		
	}
}